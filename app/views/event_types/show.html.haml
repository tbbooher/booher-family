- title "Event Type"

%p
  %strong Name:
  %br
  = @event_type.name

.form-actions
  = link_to t('.back', :default => t("helpers.links.back")), event_types_path, :class => 'btn'
  = link_to t('.edit', :default => t("helpers.links.edit")), edit_event_type_path(@event_type), :class => 'btn'
  = link_to t('.destroy', :default => t("helpers.links.destroy")), event_type_path(@event_type), :method => "delete", :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-danger'
